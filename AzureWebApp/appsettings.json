{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "system.webServer": {
    "applicationInitialization": [
      {
        "initializationPage": "/",
        "hostName": "[app hostname]"
      },
      {
        "initializationPage": "/Home/About",
        "hostName": "[app hostname]"
      }
    ]
  }
}
